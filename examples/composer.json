{
    "description" : "The CodeIgniter Application with Composer",
    "repositories": [
        {
            "type": "vcs",
            "url": "../",
            "options": {
                "symlink": true
            }
        }
    ],
    "scripts": {
        "deploy": "cp -r ../src/X/* vendor/takuya-motoshima/codeigniter-extension/src/X/"
    },
    "require": {
        "php": "^7.0",
        "takuya-motoshima/codeigniter-extension": "dev-master"
    }
}
